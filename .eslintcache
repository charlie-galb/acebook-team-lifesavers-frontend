\n  }\n\n  componentDidMount() {\n    this.checkLoginStatus();\n  }\n\n  handleLogin(data) {\n    this.setState({\n      loggedInStatus: \"LOGGED_IN\",\n      user: data.user,\n    });\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        <BrowserRouter>\n          <Switch>\n            <Route\n              exact\n              path={\"/\"}\n              render={(props) => (\n                <Home\n                  {...props}\n                  handleLogin={this.handleLogin}\n                  loggedInStatus={this.state.loggedInStatus}\n                />\n              )}\n            />\n            <Route\n              exact\n              path={\"/dashboard\"}\n              render={(props) => (\n                <Dashboard\n                  {...props}\n                  loggedInStatus={this.state.loggedInStatus}\n                />\n              )}\n            />\n          </Switch>\n        </BrowserRouter>\n        {/* <Posts posts={this.state.posts} /> */}\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/components/auth/Registration.js",[],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/components/auth/Login.js",[],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/components/PostItem.js",[],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/index.js",[],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/reportWebVitals.js",[],["41","42"],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/components/Posts.js",[],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/components/Home.js",[],"/Users/emily-alicemsesto/Documents/Projects/week-8/acebook-team-lifesavers-frontend/src/components/Dashboard.js",["43"],{"rule